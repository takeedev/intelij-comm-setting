<keymap version="1" name="Windows copy" parent="$default">
  <action id="ActivateBookmarksToolWindow" />
  <action id="ActivateFindToolWindow" />
  <action id="ActivateMavenToolWindow">
    <keyboard-shortcut first-keystroke="alt 2" />
  </action>
  <action id="ActivateTerminalToolWindow">
    <keyboard-shortcut first-keystroke="alt 3" />
  </action>
  <action id="MoveTabRight">
    <keyboard-shortcut first-keystroke="shift ctrl alt a" />
  </action>
  <action id="NextSplitter">
    <keyboard-shortcut first-keystroke="shift ctrl alt open_bracket" />
  </action>
  <action id="NextTab">
    <keyboard-shortcut first-keystroke="alt right" />
    <keyboard-shortcut first-keystroke="alt l" />
  </action>
  <action id="PrevSplitter">
    <keyboard-shortcut first-keystroke="shift ctrl alt close_bracket" />
  </action>
  <action id="PreviousTab">
    <keyboard-shortcut first-keystroke="alt left" />
    <keyboard-shortcut first-keystroke="alt h" />
  </action>
  <action id="SplitVertically">
    <keyboard-shortcut first-keystroke="shift ctrl alt q" />
  </action>
  <action id="ToggleZenMode">
    <keyboard-shortcut first-keystroke="shift slash" />
  </action>
  <action id="Unsplit">
    <keyboard-shortcut first-keystroke="shift ctrl alt z" />
  </action>
  <action id="ViewToolBar">
    <keyboard-shortcut first-keystroke="shift alt 2" />
  </action>
  <action id="ViewToolButtons">
    <keyboard-shortcut first-keystroke="shift alt 1" />
  </action>
  <action id="VimPluginToggle">
    <keyboard-shortcut first-keystroke="shift alt v" />
  </action>
</keymap>